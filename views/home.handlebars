<header>{{!--header or div in nested handlebars?--}}
    <h1>Welcome {{user}}</h1>
</header>
    <section class="feed"> {{!--section or div?--}}
    {{#each recipes as |recipe|}}
        <div class="post-card"> {{!--recipe feed cards--}}
            <h2>{{recipe.name}}</h2>
            <p>{{recipe.description}}</p>
            <p class="author">By: {{recipe.user.first_name}} {{recipe.user.last_name}}</p>
        </div>
    {{/each}}
    </section>

<script src="/js/home.js"></script>